{
    "name":"fraserreed/meme-puush",
    "type":"library",
    "description":"Generate a meme to a local file, echo to the screen, or upload to puu.sh using their API",
    "keywords":["meme", "puush", "puu.sh"],
    "license":"MIT",
    "authors":[
        {
            "name":"Fraser Reed",
            "email":"fraser.reed@gmail.com"
        }
    ],
    "require":{
        "php":">=5.3.3",
        "guzzle/guzzle":"~3.8"
    },

    "autoload":{
        "psr-0":{
            "MemePuush\\Tests":"tests/",
            "MemePuush":"src/"
        }
    },

    "require-dev":{
        "phpunit/phpunit":"3.7.*",
        "satooshi/php-coveralls":"dev-master"
    }
}
